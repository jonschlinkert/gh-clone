{
  "name": "gh-clone",
  "description": "git clone a repo with only username/repo, or if it's an NPM package you can use just the name of the project.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/gh-clone",
  "author": "Jon Schlinkert (http://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "John Otander (http://johnotander.com)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Matheus Alves (https://github.com/theuves)"
  ],
  "repository": "jonschlinkert/gh-clone",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gh-clone/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "gh-clone": "./bin/clone.js",
    "clone": "./bin/clone.js"
  },
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "any-shell-escape": "^0.1.1",
    "cross-spawn": "^5.1.0",
    "each-series-async": "^1.0.1",
    "get-repository-url": "^0.1.1",
    "is-git-url": "^1.0.0",
    "isobject": "^3.0.1",
    "log-ok": "^0.1.1",
    "minimist": "^1.2.0",
    "once": "^1.4.0"
  },
  "devDependencies": {
    "mocha": "^3.5.3",
    "delete": "^1.1.0",
    "gulp-format-md": "^1.0.0"
  },
  "keywords": [
    "clone",
    "download",
    "gh",
    "git",
    "github",
    "repo"
  ],
  "verb": {
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "first-commit-date",
        "get-first-commit",
        "github-base"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb"
    ]
  },
  "lintDeps": {
    "devDependencies": {
      "files": {
        "patterns": [
          "exec.js",
          "spawn.js"
        ]
      }
    }
  }
}
