{
  "name": "gh-clone",
  "description": "git clone a repo with only username/repo.",
  "version": "0.4.1",
  "homepage": "https://github.com/jonschlinkert/gh-clone",
  "author": "Jon Schlinkert (http://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gh-clone",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gh-clone/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "bin/"
  ],
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "test": "mocha"
  },
  "preferGlobal": true,
  "bin": {
    "gh-clone": "./bin/clone.js"
  },
  "dependencies": {
    "cross-spawn": "^4.0.0",
    "extend-shallow": "^2.0.1",
    "get-pkg": "^0.3.0",
    "log-utils": "^0.1.4",
    "minimist": "^1.2.0",
    "relative": "^3.0.2"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "clone",
    "download",
    "git",
    "github",
    "repo"
  ],
  "verb": {
    "related": {
      "list": [
        "first-commit-date",
        "get-first-commit",
        "github-base"
      ]
    },
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb"
    ]
  }
}
